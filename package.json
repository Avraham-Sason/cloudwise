    "description": "responsible for all akeyless cache-server",
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "start": "nodemon --exec ts-node src/app.ts",
        "startjs": "tsc&&node dist/app.js",
        "git": "powershell -ExecutionPolicy Bypass -File ./ps/create_branch.ps1",
