        "git2": "powershell -ExecutionPolicy Bypass -File ./ps/move_to_develop.ps1",
        "deploy": "powershell -ExecutionPolicy Bypass -File ./ps/deployment.ps1",
        "deployAll": "powershell -ExecutionPolicy Bypass -File ./ps/deployAll.ps1",
        "switch": "powershell -ExecutionPolicy Bypass -File ./ps/switch_project.ps1",
